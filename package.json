{
  "name": "naotimes-panel",
  "version": "0.1.0",
  "description": "A web panel for naoTimes Showtimes Module",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm-run-all -p build:src build:css",
    "build:src": "swc src -d dist",
    "build:css": "cross-env NODE_ENV=production postcss -o public/assets/main.css src/**/*.css",
    "build:css:dev": "postcss -o public/assets/main.css src/**/*.css",
    "prod": "pnpm run build && pnpm run prod:start",
    "prod:start": "node .",
    "dev": "pnpm run build:css:dev && pnpm run dev:start",
    "dev:start": "ts-node --transpile-only ./src/index.ts",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "testall": "npm-run-all lint type-check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/naoTimesdev/webpanel.git"
  },
  "keywords": [
    "nextjs"
  ],
  "author": "N4O",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/naoTimesdev/webpanel/issues"
  },
  "homepage": "https://github.com/naoTimesdev/webpanel#readme",
  "dependencies": {
    "@tailwindcss/forms": "^0.3.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "connect-ensure-login": "^0.1.1",
    "connect-flash": "^0.1.1",
    "connect-redis": "^5.1.0",
    "consolidate": "^0.16.0",
    "cookie": "^0.4.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.6",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-winston": "^4.1.0",
    "ioredis": "^4.24.5",
    "lodash.get": "^4.4.2",
    "lodash.has": "^4.5.2",
    "mongoose": "^5.12.2",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "ts-mongoose": "^0.0.24",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.36",
    "@swc/core": "^1.2.50",
    "@tailwindcss/jit": "^0.1.10",
    "@types/body-parser": "^1.19.0",
    "@types/compression": "^1.7.0",
    "@types/connect-ensure-login": "^0.1.5",
    "@types/connect-flash": "^0.0.36",
    "@types/connect-redis": "^0.0.16",
    "@types/consolidate": "^0.14.0",
    "@types/cookie": "^0.4.0",
    "@types/cors": "^2.8.10",
    "@types/ejs": "^3.0.6",
    "@types/express-session": "^1.17.3",
    "@types/ioredis": "^4.22.1",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.has": "^4.5.6",
    "@types/mongoose": "^5.10.4",
    "@types/node": "^14.14.36",
    "@types/passport-local": "^1.0.33",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "autoprefixer": "^10.2.5",
    "cross-env": "^7.0.3",
    "cssnano": "^4.1.10",
    "eslint": "^7.19.0",
    "eslint-config-prettier": "^7.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.8",
    "postcss-cli": "^8.3.1",
    "prettier": "^2.0.2",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "tailwindcss": "^2.0.4",
    "tailwindcss-cli": "^0.1.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  }
}
